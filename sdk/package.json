{
  "name": "@m0-foundation/solana-m-sdk",
  "version": "0.1.0",
  "description": "Solana M SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "dts": true,
    "sourcemap": true,
    "clean": true
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "M0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m0-foundation/solana-m.git"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.87.6",
    "bn.js": "^5.2.1",
    "bs58": "^6.0.0",
    "graphql": "^16.10.0",
    "graphql-request": "6.1.0",
    "js-sha256": "^0.11.0",
    "limiter": "^3.0.0",
    "sha3": "^2.1.4",
    "viem": "^2.23.14",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.6",
    "@types/node": "^20.10.0",
    "tsup": "^8.4.0",
    "typescript": "^5.3.2"
  }
}
