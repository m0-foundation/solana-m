{
  "name": "@m0-foundation/solana-m-sdk",
  "version": "0.2.1",
  "description": "Solana M SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "splitting": false,
    "dts": true,
    "sourcemap": true,
    "clean": true
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "M0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m0-foundation/solana-m.git"
  },
  "dependencies": {
    "@coral-xyz/anchor": "catalog:",
    "@m0-foundation/solana-m-api-sdk": "workspace:^",
    "@solana/spl-token": "catalog:",
    "@solana/web3.js": "catalog:",
    "@types/bn.js": "catalog:",
    "bn.js": "catalog:",
    "bs58": "catalog:",
    "js-sha256": "^0.11.0",
    "limiter": "catalog:",
    "sha3": "catalog:",
    "viem": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "catalog:"
  }
}
